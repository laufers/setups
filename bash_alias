# csh aliases

alias al='alias'
alias ami='whoami'
alias bye='exit'
alias ccore='find $HOME -name core -print -exec /usr/bin/rm {} \;'
alias chkrpc='rpcinfo -p | sed -n -e 1p -e /status/p'
alias clr='clear'
alias cls='clear'
alias cp='cp -i'
alias del='rm -i'
alias df='df -h'
alias dir='ls -F'
alias dirm='ls -F |more'
alias drop='cd ~/Dropbox'
alias dropbox='cd ~/Dropbox/'
alias fcore='find . -name "core" -print'
alias follinks='set symlinks = ignore'
alias fu='chkuser'
alias garp='garp -output 1 &'
alias gemcolor='less ${HOME}/gem/gemcolor'
alias get='git'
alias gita='git add'
alias gitb='git branch'
alias gitch='git checkout'
alias gitcom='git checkout'
alias gits='git status'
alias h='history 30'
alias http='cd ${HOME}/http'
alias idv='cd /Applications/idv;java -Xmx512m -jar idv.jar'
alias idv2='cd /Applications/idv;java -Xmx1024m -jar idv.jar'
alias idv3='cd /Applications/idv;java -Xmx1536m -jar idv.jar'
alias idv4='cd /Applications/idv;java -Xmx2048m -jar idv.jar'
alias idv5='cd /Applications/idv;java -Xmx4096m -jar idv.jar'
alias ip='ipython qtconsole '
alias j='jobs -l'
alias k9='kill -9'
alias la='(ls -Fa)'
alias last1='last | head 15'
alias less='less -X'
alias lf='ls -F'
alias ll='ls -l'
alias lo='exit'
alias ls='/bin/ls -G'
alias lst='ls -alt | more'
alias lst10='ls -alt | head -10'
alias lst5='ls -alt | head -5'
alias lstr='ls -altr '
alias ltar='tar tf'
alias meso='cd ${HOME}/gem/meso'
alias mroe='more'
alias mv='mv -i'
alias mywww='cd ${HOME}/http'
alias nawips='cd ~nawips/'
alias nmap='sudo nmap -sS '
alias nolinks='set symlinks = chase'
alias notebook='ipython notebook '
alias ns='nslookup -sil'
alias nts='clear;ls *nts'
alias precmd='source ~/bin/setprompt'
alias ps1='/usr/ucb/ps'
alias psm='ps aux | less'
alias psu='ps u'
alias quota='quota -v'
alias r='source'
alias restart='source ~/.cshrc'
alias rm='/bin/rm -i'
alias rmall='/bin/rm'
alias rmcore='find . -name core -print -exec \rm {} \;'
alias rmcur='find . -name ".curator_*" -print -exec \rm {} \;'
alias rmds='find . -name ".DS_Store" -print -exec \rm {} \;'
alias sgem='source /usr/local/nawips/Gemenviron'
alias sl='ls'
alias space='du -sk *| sort -nr - | less'
alias swc='cd ~/teaching/swc'
alias tail='tail -100'
alias tailf='\tail -f '
alias tim='date -u; date'
alias top10='/bin/ps aux |head -10'
alias tpop='tailf /var/log/popper.log'
alias u='uptime'
alias untar='tar xfzv'
alias vpnstart='scutil --nc start "OU VPN"'
alias vpnstop='scutil --nc stop "OU VPN"'
alias vt100='setenv TERM vt100'
alias ws='w |sort |less'
alias wwwerr='\tail -f ~www/apache/logs/error_log'
alias wwwl='cd ~www/apache/logs'
alias wwwme='cd ~laufers/http'
alias x='chmod u+x'
